<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/sign_in"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/tvRegistro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="false"
        android:editable="false"


        android:fontFamily="@font/open_sans_light"
        android:text="Registrate para ingresar"
        android:shadowColor="@color/white_wheels"
        android:textColor="@color/white_wheels"
        android:textSize="26sp"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="75dp" />


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:fontFamily="@font/open_sans_light"
        android:text="Escribe tu correo institucional"
        android:textColor="@color/white_wheels_clar"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/tvRegistro"
        tools:layout_editor_absoluteY="201dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/open_sans_light"
        android:text="Nombre de Usuario"
        android:textColor="@color/blue_wheels"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="177dp" />

    <EditText
        android:id="@+id/correoInsti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/background_input"
        android:drawableEnd="@drawable/ic_baseline_email_24"
        android:ems="11"
        android:fontFamily="@font/open_sans_light"
        android:inputType="textPersonName"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingBottom="10dp"
        android:text="Name"
        android:textColor="@color/blue_wheels"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="235dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/open_sans_light"
        android:text="ContraseÃ±a"
        android:textColor="@color/blue_wheels"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="288dp" />
    +

    <EditText
        android:id="@+id/passwordEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/background_input"
        android:drawableEnd="@drawable/ic_baseline_password_24"
        android:ems="13"
        android:inputType="numberPassword"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingBottom="10dp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="319dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/open_sans_light"
        android:text="Universidad"
        android:textColor="@color/blue_wheels"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="386dp" />

    <MultiAutoCompleteTextView
        android:id="@+id/multiAutoCompleteTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MultiAutoCompleteTextView"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="418dp" />
    +


</androidx.constraintlayout.widget.ConstraintLayout>